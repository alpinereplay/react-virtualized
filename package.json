{
    "name": "react-virtualized",
    "description": "React components for efficiently rendering large, scrollable lists and tabular data",
    "author": "Brian Vaughn <brian.david.vaughn@gmail.com>",
    "user": "bvaughn",
    "version": "9.22.3",
    "homepage": "https://github.com/bvaughn/react-virtualized",
    "main": "dist/commonjs/index.js",
    "module": "dist/es/index.js",
    "jsnext:main": "dist/es/index.js",
    "license": "MIT",
    "scripts": {
        "build": "npm run clean && npm run build:es && npm run build:commonjs && npm run build:css",
        "build:es": "tsc --target esnext --module commonjs --outDir dist/es",
        "build:commonjs": "tsc --target esnext --module commonjs --outDir dist/commonjs",
        "build:css": "postcss src/styles.css -o dist/styles.css",
        "clean": "rimraf dist",
        "test": "jest",
        "test:jest": "jest --no-watchman --runInBand",
        "test:coverage": "jest --no-watchman --maxWorkers 2 --coverage && codecov"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "files": [
        "dist",
        "dist/styles.css"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/bvaughn/react-virtualized.git"
    },
    "keywords": [
        "react",
        "reactjs",
        "react-component",
        "virtual",
        "list",
        "scrolling",
        "infinite",
        "virtualized",
        "table",
        "fixed",
        "header",
        "flex",
        "flexbox",
        "grid",
        "spreadsheet"
    ],
    "bugs": {
        "url": "https://github.com/bvaughn/react-virtualized/issues"
    },
    "devDependencies": {
        "@types/jest": "^28.1.6",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "postcss": "^8.4.14",
        "postcss-cli": "^10.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^28.0.7",
        "typescript": "^4.7.4"
    },
    "peerDependencies": {
        "react": "^15.3.0 || ^16.0.0-alpha || ^17.0.0 || ^18.0.0",
        "react-dom": "^15.3.0 || ^16.0.0-alpha || ^17.0.0 || ^18.0.0"
    },
    "browserify": {
        "transform": [
            "loose-envify"
        ]
    },
    "dependencies": {
        "dom-helpers": "^5.2.1"
    }
}
